{
  "name": "servenear",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "build:web:production": "expo export --platform web --clear",
    "preview:web": "npx serve dist",
    "deploy": "expo export --platform web --clear && echo 'Build complete! Upload the dist/ folder to your hosting provider.'",
    "deploy:netlify": "expo export --platform web --clear && netlify deploy --prod --dir=dist",
    "deploy:vercel": "expo export --platform web --clear && vercel --prod",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
