    "@react-navigation/native": "^7.0.14",
    "@ronradtke/react-native-markdown-display": "^8.1.0",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "53.0.13",
    "expo-audio": "~0.4.7",
